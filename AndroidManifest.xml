<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.TitTacToe"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="17"
              android:targetSdkVersion="21"/>
    <application android:label="@string/ticTacHead"
                 android:icon="@drawable/tic"
                 android:allowBackup="true">
        <activity android:name=".MainActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait">
            <intent-filter>

                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
</activity>

        <activity android:name=".PlayNewGame"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".PlayActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".HighScoreActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".GameOverActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".PauseActivity"
                  android:screenOrientation="portrait">
        </activity>

    </application>
</manifest>
